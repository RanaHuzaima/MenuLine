{
    "compilerOptions": {
      "target": "ES2021",
      "noImplicitAny": false,
      "lib": ["ES2021"],
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": ["src/**/*", "src/webhook"],
    "exclude": ["node_modules", "**/*.spec.ts"]
  }
  